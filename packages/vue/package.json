{
  "name": "@bootstrap-vue-plus/icons-vue",
  "version": "0.0.0-alpha.1",
  "description": "Vue components of Bootstrap Vue Plus Icons collection.",
  "type": "module",
  "keywords": [
    "icon",
    "svg",
    "vue",
    "bootstrap-vue-plus"
  ],
  "license": "MIT",
  "homepage": "https://bootstrap-vue-plus.com/",
  "bugs": {
    "url": "https://github.com/bootstrap-vue-plus/bootstrap-vue-plus-icons/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bootstrap-vue-plus/bootstrap-vue-plus-icons.git",
    "directory": "packages/vue"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./global": {
      "require": "./dist/global.cjs",
      "import": "./dist/global.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "unpkg": "dist/index.iife.min.js",
  "jsdelivr": "dist/index.iife.min.js",
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run build:generate && run-p build:build build:types",
    "build:generate": "tsx build/generate.ts",
    "build:build": "tsx build/build.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@bootstrap-vue-plus/icons-svg": "workspace:*",
    "@pnpm/find-workspace-dir": "^4.0.3",
    "@pnpm/find-workspace-packages": "^4.0.43",
    "@pnpm/logger": "^4.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.16.19",
    "@types/prettier": "^2.7.3",
    "camelcase": "^7.0.1",
    "chalk": "^5.3.0",
    "consola": "^2.15.3",
    "esbuild": "^0.18.11",
    "esbuild-plugin-globals": "^0.2.0",
    "fast-glob": "^3.3.0",
    "fs-extra": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "tsx": "^3.12.7",
    "typescript": "^4.9.5",
    "unplugin-vue": "^2.2.1",
    "vue": "^3.3.4",
    "vue-tsc": "^0.40.13"
  }
}
